{
  "name": "search-tabs-bookmarks-and-history",
  "version": "0.4.1",
  "description": "Browser extension to search and navigate browser tabs, local bookmarks and history.",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "bulma": "^0.9.3",
    "node-sass": "^6.0.1"
  },
  "scripts": {
    "build": "npm run css-build && npm run create-dist",
    "create-dist": "sh bin/createDist.sh",
    "css-build": "node-sass sass/popup.scss popup/popup.css --omit-source-map-url --output-style compressed",
    "css-watch": "npm run css-build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fannon/search-tabs-bookmarks-and-history.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "search",
    "fuzzy",
    "search",
    "history",
    "bookmarks"
  ],
  "author": "Simon Heimler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fannon/search-tabs-bookmarks-and-history/issues"
  },
  "homepage": "https://github.com/Fannon/search-tabs-bookmarks-and-history#readme"
}
