{
  "name": "Bookmark and History Search",
  "description": "Fuzzy search your bookmarks and history",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": [
    "commands",
    "bookmarks",
    "history"
  ],
  "action": {
    "default_popup": "/popup/index.html",
    "default_icon": {
      "16": "/images/logo-16.png",
      "32": "/images/logo-32.png",
      "48": "/images/logo-48.png",
      "128": "/images/logo-128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Period",
        "mac": "Command+Period"
      }
    }
  },
  "icons": {
    "16": "/images/logo-16.png",
    "32": "/images/logo-32.png",
    "48": "/images/logo-48.png",
    "128": "/images/logo-128.png"
  },
  "options_page": "/options/index.html"
}
