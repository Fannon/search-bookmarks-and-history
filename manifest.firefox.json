{
  "name": "Search Tabs, Bookmarks and History",
  "description": "Browser extension to (fuzzy) search and navigate browser tabs, local bookmarks and history.",
  "homepage_url": "https://github.com/Fannon/search-tabs-bookmarks-and-history",
  "author": "Simon Heimler",
  "version": "0.9.4",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "{bd5cec91-8853-40d9-aa80-8388a4544bd3}",
      "strict_min_version": "75.0"
    }
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "/images/logo-16.png",
      "32": "/images/logo-32.png",
      "48": "/images/logo-48.png",
      "128": "/images/logo-128.png"
    },
    "default_popup": "/popup/index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Period",
        "mac": "Command+Shift+Period"
      }
    }
  },
  "icons": {
    "16": "/images/logo-16.png",
    "32": "/images/logo-32.png",
    "48": "/images/logo-48.png",
    "128": "/images/logo-128.png"
  }
}
