{
  "cSpell.words": [
    "BULMA",
    "flexsearch",
    "fusejs",
    "fuzzyness",
    "Heimler",
    "jsyaml",
    "newtab",
    "readonly",
    "tagify",
    "webextensions",
    "yaireo"
  ],
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
